{
  "cli": {
    "version": ">= 16.12.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "development-simple": {
      "developmentClient": false,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "development-fixed": {
      "developmentClient": false,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview": {
      "developmentClient": false,
      "distribution": "internal",
      "cache": {
        "disabled": true
      },
      "prebuildCommand": "npx expo prebuild --clean --platform android",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "cache": {
        "disabled": true
      },
      "prebuildCommand": "npx expo prebuild --clean --platform android",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "production",
        "EXPO_PUBLIC_FIREBASE_API_KEY": "${EXPO_PUBLIC_FIREBASE_API_KEY}",
        "EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN": "${EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN}",
        "EXPO_PUBLIC_FIREBASE_PROJECT_ID": "${EXPO_PUBLIC_FIREBASE_PROJECT_ID}",
        "EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET": "${EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET}",
        "EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID": "${EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID}",
        "EXPO_PUBLIC_FIREBASE_APP_ID": "${EXPO_PUBLIC_FIREBASE_APP_ID}",
        "EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID": "${EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID}",
        "EXPO_PUBLIC_HELIUS_API_KEY": "${EXPO_PUBLIC_HELIUS_API_KEY}",
        "EXPO_PUBLIC_FORCE_MAINNET": "true",
        "EXPO_PUBLIC_DEV_NETWORK": "mainnet",
        "EXPO_PUBLIC_COMPANY_WALLET_ADDRESS": "${EXPO_PUBLIC_COMPANY_WALLET_ADDRESS}",
        "EXPO_PUBLIC_COMPANY_WALLET_SECRET_KEY": "${EXPO_PUBLIC_COMPANY_WALLET_SECRET_KEY}",
        "EXPO_PUBLIC_COMPANY_MIN_SOL_RESERVE": "${EXPO_PUBLIC_COMPANY_MIN_SOL_RESERVE}",
        "EXPO_PUBLIC_COMPANY_GAS_FEE_ESTIMATE": "${EXPO_PUBLIC_COMPANY_GAS_FEE_ESTIMATE}",
        "EXPO_PUBLIC_COMPANY_FEE_PERCENTAGE": "${EXPO_PUBLIC_COMPANY_FEE_PERCENTAGE}",
        "EXPO_PUBLIC_COMPANY_MIN_FEE": "${EXPO_PUBLIC_COMPANY_MIN_FEE}",
        "EXPO_PUBLIC_COMPANY_MAX_FEE": "${EXPO_PUBLIC_COMPANY_MAX_FEE}",
        "EXPO_PUBLIC_FEE_SEND_PERCENTAGE": "${EXPO_PUBLIC_FEE_SEND_PERCENTAGE}",
        "EXPO_PUBLIC_FEE_SEND_MIN": "${EXPO_PUBLIC_FEE_SEND_MIN}",
        "EXPO_PUBLIC_FEE_SEND_MAX": "${EXPO_PUBLIC_FEE_SEND_MAX}",
        "EXPO_PUBLIC_FEE_RECEIVE_PERCENTAGE": "${EXPO_PUBLIC_FEE_RECEIVE_PERCENTAGE}",
        "EXPO_PUBLIC_FEE_RECEIVE_MIN": "${EXPO_PUBLIC_FEE_RECEIVE_MIN}",
        "EXPO_PUBLIC_FEE_RECEIVE_MAX": "${EXPO_PUBLIC_FEE_RECEIVE_MAX}",
        "EXPO_PUBLIC_FEE_SPLIT_PERCENTAGE": "${EXPO_PUBLIC_FEE_SPLIT_PERCENTAGE}",
        "EXPO_PUBLIC_FEE_SPLIT_MIN": "${EXPO_PUBLIC_FEE_SPLIT_MIN}",
        "EXPO_PUBLIC_FEE_SPLIT_MAX": "${EXPO_PUBLIC_FEE_SPLIT_MAX}",
        "EXPO_PUBLIC_FEE_SETTLEMENT_PERCENTAGE": "${EXPO_PUBLIC_FEE_SETTLEMENT_PERCENTAGE}",
        "EXPO_PUBLIC_FEE_SETTLEMENT_MIN": "${EXPO_PUBLIC_FEE_SETTLEMENT_MIN}",
        "EXPO_PUBLIC_FEE_SETTLEMENT_MAX": "${EXPO_PUBLIC_FEE_SETTLEMENT_MAX}",
        "EXPO_PUBLIC_FEE_WITHDRAW_PERCENTAGE": "${EXPO_PUBLIC_FEE_WITHDRAW_PERCENTAGE}",
        "EXPO_PUBLIC_FEE_WITHDRAW_MIN": "${EXPO_PUBLIC_FEE_WITHDRAW_MIN}",
        "EXPO_PUBLIC_FEE_WITHDRAW_MAX": "${EXPO_PUBLIC_FEE_WITHDRAW_MAX}",
        "EXPO_PUBLIC_FEE_DEPOSIT_PERCENTAGE": "${EXPO_PUBLIC_FEE_DEPOSIT_PERCENTAGE}",
        "EXPO_PUBLIC_FEE_DEPOSIT_MIN": "${EXPO_PUBLIC_FEE_DEPOSIT_MIN}",
        "EXPO_PUBLIC_FEE_DEPOSIT_MAX": "${EXPO_PUBLIC_FEE_DEPOSIT_MAX}",
        "EXPO_PUBLIC_FEE_PAYMENT_REQUEST_PERCENTAGE": "${EXPO_PUBLIC_FEE_PAYMENT_REQUEST_PERCENTAGE}",
        "EXPO_PUBLIC_FEE_PAYMENT_REQUEST_MIN": "${EXPO_PUBLIC_FEE_PAYMENT_REQUEST_MIN}",
        "EXPO_PUBLIC_FEE_PAYMENT_REQUEST_MAX": "${EXPO_PUBLIC_FEE_PAYMENT_REQUEST_MAX}",
        "EXPO_PUBLIC_FEE_GROUP_PAYMENT_PERCENTAGE": "${EXPO_PUBLIC_FEE_GROUP_PAYMENT_PERCENTAGE}",
        "EXPO_PUBLIC_FEE_GROUP_PAYMENT_MIN": "${EXPO_PUBLIC_FEE_GROUP_PAYMENT_MIN}",
        "EXPO_PUBLIC_FEE_GROUP_PAYMENT_MAX": "${EXPO_PUBLIC_FEE_GROUP_PAYMENT_MAX}",
        "EXPO_PUBLIC_FEE_PREMIUM_PERCENTAGE": "${EXPO_PUBLIC_FEE_PREMIUM_PERCENTAGE}",
        "EXPO_PUBLIC_FEE_PREMIUM_MIN": "${EXPO_PUBLIC_FEE_PREMIUM_MIN}",
        "EXPO_PUBLIC_FEE_PREMIUM_MAX": "${EXPO_PUBLIC_FEE_PREMIUM_MAX}",
        "EXPO_PUBLIC_GOOGLE_CLIENT_ID": "${EXPO_PUBLIC_GOOGLE_CLIENT_ID}",
        "EXPO_PUBLIC_GOOGLE_CLIENT_SECRET": "${EXPO_PUBLIC_GOOGLE_CLIENT_SECRET}",
        "ANDROID_GOOGLE_CLIENT_ID": "${ANDROID_GOOGLE_CLIENT_ID}",
        "IOS_GOOGLE_CLIENT_ID": "${IOS_GOOGLE_CLIENT_ID}",
        "EXPO_PUBLIC_APPLE_CLIENT_ID": "${EXPO_PUBLIC_APPLE_CLIENT_ID}",
        "EXPO_PUBLIC_APPLE_SERVICE_ID": "${EXPO_PUBLIC_APPLE_SERVICE_ID}",
        "EXPO_PUBLIC_APPLE_TEAM_ID": "${EXPO_PUBLIC_APPLE_TEAM_ID}",
        "EXPO_PUBLIC_APPLE_KEY_ID": "${EXPO_PUBLIC_APPLE_KEY_ID}",
        "EXPO_PUBLIC_TWITTER_CLIENT_ID": "${EXPO_PUBLIC_TWITTER_CLIENT_ID}",
        "EXPO_PUBLIC_TWITTER_CLIENT_SECRET": "${EXPO_PUBLIC_TWITTER_CLIENT_SECRET}",
        "MOONPAY_API_KEY": "${MOONPAY_API_KEY}",
        "MOONPAY_SECRET_KEY": "${MOONPAY_SECRET_KEY}",
        "MOONPAY_WEBHOOK_SECRET": "${MOONPAY_WEBHOOK_SECRET}",
        "JWT_SECRET": "${JWT_SECRET}",
        "JWT_EXPIRES_IN": "${JWT_EXPIRES_IN}",
        "REFRESH_TOKEN_EXPIRES_IN": "${REFRESH_TOKEN_EXPIRES_IN}",
        "EMAIL_SERVICE": "${EMAIL_SERVICE}",
        "EMAIL_USER": "${EMAIL_USER}",
        "EMAIL_PASS": "${EMAIL_PASS}",
        "EMAIL_FROM": "${EMAIL_FROM}",
        "SENTRY_DSN": "${SENTRY_DSN}",
        "SENTRY_TRACES_SAMPLE_RATE": "${SENTRY_TRACES_SAMPLE_RATE}",
        "LOG_LEVEL": "${LOG_LEVEL}",
        "LOG_FORMAT": "${LOG_FORMAT}",
        "ANALYTICS_ENABLED": "${ANALYTICS_ENABLED}",
        "FIREBASE_SERVER_KEY": "${FIREBASE_SERVER_KEY}",
        "FIREBASE_PROJECT_ID": "${FIREBASE_PROJECT_ID}",
        "PUSH_NOTIFICATIONS_ENABLED": "${PUSH_NOTIFICATIONS_ENABLED}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
