# GitHub Actions Secrets for Local Testing
# Copy this file to .secrets and fill in your actual values
# DO NOT commit .secrets to version control

# GitHub Token (optional - for private repos)
GITHUB_TOKEN=your_github_token_here

# Firebase Configuration (for testing)
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id

# Solana Configuration
HELIUS_API_KEY=your_helius_api_key_here

# Security Scanning (optional)
SNYK_TOKEN=your_snyk_token_here

# Deployment (optional - for full testing)
FIREBASE_SERVICE_ACCOUNT=your_service_account_json_here
SLACK_WEBHOOK_URL=your_slack_webhook_here
